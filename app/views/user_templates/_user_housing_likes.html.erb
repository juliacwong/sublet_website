<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Housing Likes

    <a data-toggle="collapse" href="#user_housing_likes_new_form">
      Add housing like
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @user.housing_likes.each do |housing_like| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <!-- Show link start -->
        <a href="/housing_likes/<%= housing_like.id %>">
          <%= housing_like.sublessee_id %>
        </a>
        <!-- Show link end -->

      </li>
    <% end %>

  </ul>
</div>
