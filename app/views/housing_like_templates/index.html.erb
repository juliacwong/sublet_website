<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All housing likes
    </h1>

    <a href="/housing_likes/new" class="btn btn-block btn-outline-secondary">
      Add a new housing like
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Sublessee
        </th>

        <th>
          Housing
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @housing_likes.each do |housing_like| %>
      <tr>
        <!-- Display housing_like.id start -->
        <td>
          <%= housing_like.id %>
        </td>
        <!-- Display housing_like.id end -->

        <!-- Display housing_like.sublessee_id start -->
        <td>
          <%= housing_like.sublessee_id %>
        </td>
        <!-- Display housing_like.sublessee_id end -->

        <!-- Display housing_like.housing_id start -->
        <td>
          <%= housing_like.housing_id %>
        </td>
        <!-- Display housing_like.housing_id end -->

        <!-- Display housing_like.created_at start -->
        <td>
          <%= time_ago_in_words(housing_like.created_at) %> ago
        </td>
        <!-- Display housing_like.created_at end -->

        <!-- Display housing_like.updated_at start -->
        <td>
          <%= time_ago_in_words(housing_like.updated_at) %> ago
        </td>
        <!-- Display housing_like.updated_at end -->

        <td>
          <a href="/housing_likes/<%= housing_like.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
